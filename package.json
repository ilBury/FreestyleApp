{
    "name": "products",
    "version": "0.0.1",
    "private": true,
    "description": "An SAP Fiori application.",
    "keywords": [
        "wdi",
        "ui5",
        "openui5",
        "sapui5"
    ],
    "main": "webapp/index.html",
    "dependencies": {
        "generator-easy-ui5": "^1.3.8"
    },
    "devDependencies": {
        "@babel/core": "^7.24.7",
        "@babel/preset-env": "^7.24.7",
        "@babel/register": "^7.24.6",
        "@openui5/types": "^1.125.0",
        "@sap-ux/ui5-middleware-fe-mockserver": "2",
        "@sap/ui5-builder-webide-extension": "^1.1.8",
        "@sap/ux-ui5-tooling": "1",
        "@ui5/cli": "^3.0.0",
        "@ui5/webcomponents": "^1.24.5",
        "@wdio/cli": "^8.39.0",
        "@wdio/cucumber-framework": "5.16.15",
        "@wdio/local-runner": "^8.39.0",
        "@wdio/mocha-framework": "^8.39.0",
        "@wdio/reporter": "5.15.2",
        "@wdio/spec-reporter": "^8.39.0",
        "@wdio/sync": "^5.17.0",
        "@wdio/visual-service": "^5.1.0",
        "mbt": "^1.2.18",
        "rimraf": "^3.0.2",
        "ts-node": "^10.9.2",
        "typescript": "^5.4.5",
        "ui5-task-zipper": "^0.5.0",
        "wdio-ui5-service": "^2.0.9",
        "webdriverio": "^8.39.0"
    },
    "scripts": {
        "start": "fiori run --open \"test/flpSandbox.html?sap-ui-xx-viewCache=false#appproducts-display\"",
        "start-local": "fiori run --config ./ui5-local.yaml --open \"test/flpSandbox.html?sap-ui-xx-viewCache=false#appproducts-display\"",
        "build": "ui5 build --config=ui5.yaml --clean-dest --dest dist",
        "deploy": "fiori cfDeploy",
        "deploy-config": "fiori add deploy-config",
        "start-noflp": "fiori run --open \"index.html?sap-ui-xx-viewCache=false\"",
        "start-mock": "fiori run --config ./ui5-mock.yaml --open test/mockServer.html",
        "start-variants-management": "fiori run --open \"preview.html?sap-ui-xx-viewCache=false&fiori-tools-rta-mode=true&sap-ui-rta-skip-flex-validation=true#preview-app\"",
        "unit-tests": "fiori run --config ./ui5-mock.yaml --open 'test/unit/unitTests.qunit.html'",
        "int-tests": "fiori run --config ./ui5-mock.yaml --open 'test/integration/opaTests.qunit.html'",
        "build:cf": "ui5 build preload --clean-dest --config ui5-deploy.yaml --include-task=generateCachebusterInfo",
        "build:mta": "rimraf resources mta_archives && mbt build",
        "undeploy": "cf undeploy appproducts --delete-services --delete-service-keys --delete-service-brokers",
        "wdi5": "wdio run ./webapp/test/e2e/wdio.conf.ts",
        "wdio": "wdio run ./wdio.conf.js"
    },
    "sapuxLayer": "CUSTOMER_BASE",
    "ui5": {
        "dependencies": [
            "@sap/ui5-builder-webide-extension",
            "ui5-task-zipper",
            "mbt"
        ]
    }
}

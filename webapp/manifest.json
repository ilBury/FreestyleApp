{
	"sap.app": {
		"id": "products.app",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"dataSources": {
			"default": {
				"uri": "proxy/V2/(S(3g0wqswc4au3yrwcjt9coqzw))/OData/OData.svc/",
				"type": "OData",
				"settings": {
					"odataVersion": "2.0"
				}
			}
		}
	},
	"sap.ui": {
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": [
			"sap_horizon"
		]
	},
	"sap.ui5": {
		"rootView": "products.app.view.App",
		"handleValidation": true,
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"viewPath": "products.app.view",
				"controlId": "RootApp",
				"controlAggregation": "pages",
				"bypassed": {
					"target": [
						"notFound"
					]
				},
				"async": true
			},
			"routes": [
				{
					"pattern": "",
					"name": "ListReport",
					"target": [
						"listReport"
					]
				},
				{
					"pattern": "product/{productId}/:mode:",
					"name": "ProductDetails",
					"target": [
						"productDetails"
					]
				}
			],

			"targets": {
				"listReport": {
					"viewName": "ListReport",
					"viewId": "listReport",
					"viewLevel": 1
				},

				"productDetails": {
					"viewName": "ProductDetails",
					"viewId": "productDetails",
					"viewLevel": 2
				},

				"notFound": {
					"viewName": "NotFound",
					"viewId": "notFound"
				}
			}
		},
		"dependencies": {
			"minUI5Version": "1.120.0",
			"libs": {
				"sap.m": {}
			}
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "products.app.i18n.i18n"
				}
			},
			"": {
				"dataSource": "default",
				"settings": {
					"useBatch": false,
					"defaultBindingMode": "TwoWay",
					"defaultCountMode": "Inline",
					"disableHeadRequestForToken": true
				}
			}
		}
	}
}
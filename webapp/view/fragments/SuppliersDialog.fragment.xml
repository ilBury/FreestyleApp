<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<SelectDialog
		id="valueHelpDialog"
		title="Products"
		items="{/Suppliers}"
		liveChange="_handleValueHelpSearch"
		confirm="_handleValueHelpClose"
		cancel="_handleValueHelpClose"
		multiSelect="true">
		<StandardListItem
			iconDensityAware="false"
			iconInset="false"
			title="{SuppliersName}"
			description="{Address}" />
	</SelectDialog>
</core:FragmentDefinition>